{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": "google",
  "parser": "babel-eslint",
  "rules": {
    "no-tabs": 0,
    "indent": [
      2,
      "tab",
      { "SwitchCase": 1 }
    ],
    "space-before-function-paren": "off",
    "require-jsdoc": "off",
    "object-curly-spacing": "off",
    "operator-linebreak": "off",
    "linebreak-style": "off"
  }
}
